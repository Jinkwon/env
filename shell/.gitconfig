[alias]
  co = checkout
  s = status
  amd = commit --amend
[http]
  postBuffer = 157286400
  version = HTTP/1.1
  sslVerify = true
[core]
  ignorecase = false
  excludesFile = ~/.gitignore
  quotepath = false
  precomposeunicode = true
[pull]
  rebase = false
[init]
  defaultBranch = main
[credential]
  helper = store